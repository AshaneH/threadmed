{
  "name": "threadmed",
  "version": "0.1.0",
  "description": "Literature synthesis tool for evidence-based medicine",
  "main": "./out/main/index.js",
  "author": "ThreadMed Contributors",
  "license": "MIT",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "start": "electron-vite preview",
    "rebuild": "node -e \"const v=require('./node_modules/electron/package.json').version; const {execSync}=require('child_process'); execSync('npx node-gyp rebuild --directory node_modules/better-sqlite3 --runtime=electron --target='+v+' --dist-url=https://electronjs.org/headers', {stdio:'inherit'})\"",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json",
    "typecheck": "npm run typecheck:node && npm run typecheck:web"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-table": "^8.20.0",
    "@tanstack/react-virtual": "^3.10.0",
    "better-sqlite3": "^11.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.460.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-resizable-panels": "^2.1.0",
    "tailwind-merge": "^2.5.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.0",
    "electron-vite": "^2.3.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.0"
  }
}